<view class="container">
    <view class="brand-list" >
        <navigator url="../brandDetail/brandDetail?id={{item.Id}}"  class="item" wx:for="{{brandList}}" wx:key="id">
            <view class="img-bg">
                <image src="{{FileHost+item.ImageUrl}}" background-size="cover"></image>
            </view>
            <view class="txt-box">
                <view class="line">
                    <text class="name" >{{item.Title}}</text>
                    <!--<text class="s">|</text>
                    <text class="price">{{item.Remark}}</text>-->
                </view>
            </view>
        </navigator >
    </view>
</view>