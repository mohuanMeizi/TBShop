page { 
overflow:hidden; 
position:fixed; 
background-image:url('http://api.zhaixiangyoukuai.com/Content/images/shopreg.jpg'); 
margin: 0px;
background-size: 100% 100%; 
} 

.box{
  height:400rpx;
  width:100%;
  position:absolute;
  margin-top:60%;
}

.item{
  width:100%;
  height:60rpx;
  line-height:60rpx;
  float:left;
  margin-top:20rpx;
}
.item_l{
  width:25%;
  float:left;
  text-align:right;
  color:#FFFFFF;
  padding-right:20rpx;
}
.item_r{
  width:55%;
  float:left;
  text-align:left;
}
.item_r input{
  background-color:#FFFFFF; 
  border-radius:5rpx;
  padding-left:10rpx;
}

.item_r button{
  background-color:#FFFFFF; 
  border-radius:5rpx;
  width:100%;
  padding:0;
  margin:0;
  height:60rpx;
  line-height:60rpx;
  font-size:25rpx; 
  float:left;
  text-align:left;
}

.item_r button icon{
  background-color:#FFFFFF; 
  border-radius:5rpx;
  width:13%;
  padding:0;
  margin:-10rpx 0rpx 0rpx 20rpx;
  height:40rpx;
  line-height:40rpx;
  float:left;
  font-size:40rpx;
}

.item1{
  width:100%;
  height:60rpx;
  line-height:60rpx;
  float:left;
  margin-top:40rpx;
  text-align:center;
}

.item1 button{
  width:80%;
  height:60rpx;
  line-height:60rpx;
  font-size:25rpx; 
  text-align:center;
}

.item1 button icon{
  background-color:#FFFFFF; 
  border-radius:5rpx;
  width:10%;
  padding:0;
  margin:-3rpx 0rpx 0rpx 0rpx;
  height:40rpx;
  line-height:40rpx;
  float:left;
  font-size:40rpx;
}